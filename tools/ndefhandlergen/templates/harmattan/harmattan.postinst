#!/bin/sh

dbus-send --system --type=method_call --dest=com.nokia.nfc / \
    com.nokia.nfc.Manager.RegisterNDEFHandler \
        string:session \
        string:com.nokia.qtmobility.nfc.%APPNAME% \
        objpath:/com/nokia/nfc/ndefhandler \
        string:any \
        string:"%DATATYPE%" \
        string:%APPNAME%
